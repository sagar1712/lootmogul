{
	"name": "lootmogul-backend",
	"version": "1.2.3",
	"description": "Nodejs Express Sequelize Postgres API blockbounce with JWT",
	"main": "index.js",
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"preinstall": "node generateConfig.js",
		"postinstall": "npm install pg",
		"start": "node app.js",
		"prepare": "npx sequelize-cli db:migrate",
		"start:dev": "nodemon app.js",
		"start:test": "node start --env test",
		"start:prod": "node start --env prod",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"keywords": [
		"api",
		"boilerplate",
		"nodejs",
		"express",
		"sequelize",
		"postgres",
		"jwt",
		"api-boilerplate",
		"nodejs-api",
		"nodejs-sequelize-api"
	],
	"author": "Sagar",
	"license": "ISC",
	"dependencies": {
		"axios": "^1.6.8",
		"bcrypt": "^5.1.0",
		"body-parser": "^1.20.1",
		"cors": "^2.8.5",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"helmet": "^6.0.1",
		"http-status": "^1.6.2",
		"joi": "^17.9.1",
		"jsonwebtoken": "^8.5.1",
		"moment": "^2.29.4",
		"nodemailer": "^6.9.1",
		"nodemon": "^2.0.20",
